{
  "compilerOptions": {
    "allowJs": true,
    "baseUrl": "src",
    "outDir": "dist",
    "module": "commonjs",
    "target": "es2016",
    "sourceMap": true,
    "moduleResolution": "node",
    "strict": true,
    "preserveConstEnums": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules","src/services","src/services/candles/*","src/http/transactions","src/http/candles"]
}
